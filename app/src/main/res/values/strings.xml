    <!-- PRODUCT TOUR -->
    <string name="Slide1_Heading">Layers Manager</string>
    <string name="Slide1_Text">The central place to manage all your Overlays.</string>
    <string name="Slide2_Heading">Plugin Overlays</string>
    <string name="Slide2_Text">Compatible themes are directly integrated into the Layers Manager. Open them from the main page and directly install your favorite Overlays.</string>
    <string name="Slide3_Heading">APK / ZIP Overlays</string>
    <string name="Slide3_Text">Install themes available as apk´s or zip´s by clicking on the Floating Action Button.</string>
    <string name="Slide4_Heading">Uninstall Overlays</string>
    <string name="Slide4_Text">Uninstall the Overlays you do not want to use anymore.</string>
    <string name="Slide5_Heading">Backup &amp; Restore</string>
    <string name="Slide5_Text">Backup your favorite Overlays to restore them later.</string>


    <!-- INSTRUCTIONS/TUTORIAL -->
    <string name="instructionstitle">Tutorial</string>
    <string name="instructionscard1header">Install plugin overlays</string>
    <string name="instructionscard1">Click one of the installed overlays\nChoose the apps you want to theme\nClick the FAB to install the overlays</string>
    <string name="instructionscard2header">Install normal overlays</string>
    <string name="instructionscard2">Click the FAB on the main screen\nChoose either a .zip or multiple .apk\'s\nClick the FAB to install the selected overlays</string>
    <string name="instructionscard3header">Uninstall overlays</string>
    <string name="instructionscard3">From the nav-drawer select "uninstall overlays"\nSelect multiple overlays\nClick the FAB to uninstall the selected overlays</string>
    <string name="instructionscard4header">Backup overlays</string>
    <string name="instructionscard4">From the nav-drawer select "Backup overlays"\nEnter a name and click "OK"</string>
    <string name="instructionscard5header">Restore overlays</string>
    <string name="instructionscard5">From the nav-drawer select "Restore overlays"\nClick the FAB to Restore the selected backup file</string>
    <string name="instructionscard6header">Replay Introduction</string>
    <string name="instructionscard6">Click to start tutorial from the beginning</string>

